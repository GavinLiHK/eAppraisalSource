	<queryString>
		<![CDATA[SELECT C.OVERALL_RATING,
REPLACE(C.XXOVERALL_COMMENT, utl_raw.cast_to_varchar2('F48FBFBD')) as OVERALL_COMMENT,
--20240923 Remove chr(255) from the database value
--chr(13) || chr(10) || chr(255) as OVERALL_COMMENT_SPACE,
chr(13) || chr(10) as OVERALL_COMMENT_SPACE,
--End 20240923 Remove chr(255) from the database value
REPLACE(C.SPECIAL_FACTORS, utl_raw.cast_to_varchar2('F48FBFBD')) as SPECIAL_FACTORS,
--20240923 Remove chr(255) from the database value
--chr(13) || chr(10) || chr(255) as SPECIAL_FACTORS_SPACE
chr(13) || chr(10) as SPECIAL_FACTORS_SPACE
--End 20240923 Remove chr(255) from the database value
 , LENGTHB(TO_CHAR(SUBSTR(C.XXOVERALL_COMMENT,1,1000))) - LENGTHB(TO_CHAR(SUBSTR(C.XXOVERALL_COMMENT,1,1000))) as OVERALL_COMMENT_LEN
 , lengthb(C.SPECIAL_FACTORS) - length(C.SPECIAL_FACTORS) as SPECIAL_FACTORS_LEN
FROM HA_HR_EA_CONTRACT C
WHERE C.REPORT_ID = $P{ReportID}]]>
	</queryString>
