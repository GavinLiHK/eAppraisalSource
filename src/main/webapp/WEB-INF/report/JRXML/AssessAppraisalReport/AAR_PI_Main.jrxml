	<queryString>
		<![CDATA[SELECT R.NAME, R.CHINESE_NAME, R.FIRST_APPOINTMENT_RANK, R.DATE_OF_APP_FIRST_RANK,
R.SUBSTANTIVE_RANK, R.DATE_OF_APP_SUB_RANK, R.PRESENT_POST, R.TELEPHONE, R.DATE_OF_POSTING, 
R.DIVISION,
decode(ppei.PEI_INFORMATION1, 'REG',   TO_CHAR(R.CURRENT_CONTRACT_START_DATE, 'MM/YYYY') , TO_CHAR(R.CURRENT_CONTRACT_START_DATE, 'DD/MM/YYYY')) as CURRENT_CONTRACT_START_DATE, 
decode(ppei.PEI_INFORMATION1, 'REG',   TO_CHAR(R.CURRENT_CONTRACT_END_DATE, 'MM/YYYY') , TO_CHAR(R.CURRENT_CONTRACT_END_DATE, 'DD/MM/YYYY')) as CURRENT_CONTRACT_END_DATE, 
REPLACE(C.COMMENDATION, utl_raw.cast_to_varchar2('F48FBFBD')) as COMMENDATION,
--20240923 Remove chr(255) from the database value
--chr(255) as COMMENDATION_SPACE,
' ' as COMMENDATION_SPACE,
--End 20240923 Remove chr(255) from the database value
lengthb(C.COMMENDATION) - length(C.COMMENDATION) as COMMENDATION_LEN
FROM HA_HR_EA_REPORT R, HA_HR_EA_CONTRACT C, per_all_people_f papf, per_people_extra_info ppei
WHERE R.REPORT_ID = $P{ReportID} and R.REPORT_ID=C.REPORT_ID
and r.employee_number = papf.employee_number
and papf.person_id = ppei.person_id
and ppei.information_type = 'HA_CONTRACT_INFO'
and r.current_contract_end_date between to_date(ppei.PEI_INFORMATION2, 'yyyy/mm/dd hh24:mi:ss') and to_date(ppei.PEI_INFORMATION3, 'yyyy/mm/dd hh24:mi:ss')
and rownum = 1]]>
	</queryString>
