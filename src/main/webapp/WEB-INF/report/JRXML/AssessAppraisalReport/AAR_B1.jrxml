	<queryString>
		<![CDATA[SELECT REPLACE(O.KEY_OBJECTIVE, utl_raw.cast_to_varchar2('F48FBFBD')) as KEY_OBJECTIVE,
--20240923 Remove chr(255) from the database value
--chr(13) || chr(10) || chr(255) as KEY_OBJECTIVE_SPACE,
chr(13) || chr(10) as KEY_OBJECTIVE_SPACE,
--End 20240923 Remove chr(255) from the database value
REPLACE(O.RESULTS_ACHIEVED, utl_raw.cast_to_varchar2('F48FBFBD')) as RESULTS_ACHIEVED,
--20240923 Remove chr(255) from the database value
--chr(13) || chr(10) || chr(255) as RESULTS_ACHIEVED_SPACE,
chr(13) || chr(10) as RESULTS_ACHIEVED_SPACE,
--End 20240923 Remove chr(255) from the database value
O.WEIGHTING, O.RATING
		, lengthb(KEY_OBJECTIVE) - length(KEY_OBJECTIVE) as KEY_OBJECTIVE_LEN
		, lengthb(RESULTS_ACHIEVED) - length(RESULTS_ACHIEVED) as RESULTS_ACHIEVED_LEN
FROM HA_HR_EA_CONTRACT C, HA_HR_EA_OBJECTIVES O
WHERE C.REPORT_ID = $P{ReportID}
AND C.REPORT_ID = O.REPORT_ID(+)
AND O.TYPE(+) = 'PARTB'
AND (O.KEY_OBJECTIVE IS NOT NULL OR O.RESULTS_ACHIEVED IS NOT NULL)
ORDER BY O.SEQ_NO]]>
